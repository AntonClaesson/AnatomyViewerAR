material {
    name : "yellow_opaque_material",
    shadingModel : unlit,
    blending : opaque,
}

fragment {
    void material(inout MaterialInputs material) {
        prepareMaterial(material);
        material.baseColor = vec4(1.0, 1, 0.0, 1.0);
        material.emissive = vec4(0.5, 0.5, 0.5, 1.0);
    }
}